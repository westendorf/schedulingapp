        <TopMenu fx:id="topMenu" />
    </top>
    <center>
    <AnchorPane prefHeight="400.0" prefWidth="600.0">
        <children>
            <TableView fx:id="scheduleTable" layoutY="50.0" prefHeight="298.0" prefWidth="600.0">
