                    <TableColumn fx:id="tcContact" prefWidth="75.0" text="Contact" />
                    <TableColumn fx:id="tcCustomerId" prefWidth="75.0" text="Customer ID" />
                    <TableColumn fx:id="tcAppointmentId" prefWidth="75.0" text="Appointment ID" />
                    <TableColumn fx:id="tcTitle" prefWidth="75.0" text="Title" />
                    <TableColumn fx:id="tcType" prefWidth="75.0" text="Type" />
                    <TableColumn fx:id="tcDescription" prefWidth="75.0" text="Description" />
                    <TableColumn fx:id="tcStart" prefWidth="75.0" text="Start" />
                    <TableColumn fx:id="tcEnd" prefWidth="75.0" text="End" />
                </columns>
            </TableView>
        </children>
    </AnchorPane>
